{
  "name": "dicopal",
  "version": "0.8.1",
  "description": "Discrete color palettes for cartography and data visualization",
  "homepage": "https://github.com/riatelab/dicopal.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riatelab/dicopal.js"
  },
  "bugs": {
    "url": "https://github.com/riatelab/dicopal.js/issues"
  },
  "type": "module",
  "main": "dist/index.min.js",
  "module": "dist/index.min.esm.js",
  "exports": {
    "import": "./dist/index.min.esm.js",
    "require": "./dist/index.min.js",
    "default": "./dist/index.min.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "test:coverage": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --collect-coverage"
  },
  "author": {
    "name": "Matthieu Viry",
    "email": "matthieu.viry@cnrs.fr"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "d3-array": "^3.2.4",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/d3-array": "^3.2.1",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-scale": "^4.0.8",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rollup": "^4.14.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.4"
  },
  "keywords": [
    "color",
    "palette",
    "cartography",
    "visualization"
  ]
}
